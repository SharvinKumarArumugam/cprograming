//pre-processor section
#include<stdio.h>

//global variablection 
void add(int,int);
//main functional section 
int main(){
    int a,b;
    printf("Enter The Value of A & B : ");
    scanf("%d%d",a&b);
    add(a,b);
    return 0;

}

void add(int x, int y){
    int c;
    c=x+y;

}



